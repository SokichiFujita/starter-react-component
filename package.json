{
  "name": "starter-react-component",
  "version": "1.3.2",
  "private": false,
  "preferGlobal": true,
  "bin": {
    "starter-react-component": "bin/index.js"
  },
  "scripts": {
    "pretest": "npm run cleanup && npm link",
    "test": "mkdir __test && cd __test && starter-react-component init MyComponent && npm test && npm run build",
    "posttest": "npm unlink",
    "cleanup": "rm -rf __test && rm -rf node_modules"
  },
  "description": "Create a React Component to publish as a npm module",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SokichiFujita/starter-react-component"
  },
  "keywords": [
    "react",
    "react.js",
    "reactjs",
    "npm",
    "npm publish",
    "jest",
    "starter",
    "starter kit",
    "webpack",
    "babel",
    "eslint"
  ],
  "author": "Sokichi Fujita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SokichiFujita/starter-react-component/issues"
  },
  "homepage": "https://github.com/SokichiFujita/starter-react-component"
}
